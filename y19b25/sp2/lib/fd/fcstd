        SELECT STD-FILE
	       ASSIGN TO 	DISK STD-DATANAME
	       ORGANIZATION	INDEXED
	       LOCK MODE	AUTOMATIC
	       ACCESS		DYNAMIC
	       RECORD KEY	STD-KEY
	       ALTERNATE KEY STD-ALT-KEY1 = STD-CY-KEY, STD-KEY
	       ALTERNATE KEY STD-ALT-KEY2 = STD-RC-KEY, STD-KEY
	       ALTERNATE KEY STD-ALT-KEY3 = STD-RG-KEY, STD-KEY
	       FILE STATUS	STD-STATUS.
